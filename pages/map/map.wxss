.outer-box{
  padding: 30rpx;
}
.addr-box{
  border: 1px solid #333;
}
.addr{
  line-height: 40rpx;
  padding: 0 120rpx 20rpx 0;
  position: relative;
  font-size: 28rpx;
}
.addr-none{
  line-height: 40rpx;
  padding: 0 120rpx 0 0;
  position: relative;
  font-size: 28rpx;
}
.addr-block{
  line-height: 40rpx;
  padding: 0 20rpx 20rpx 0;
  position: relative;
  font-size: 28rpx;
}
.arr{
  width: 9rpx;
  height: 16rpx;
  float: right;
  margin: 15rpx 0 0 30rpx;
}
.meter{
  height: 40rpx;
  line-height: 40rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.txt-box{
  /* border-bottom: 1px solid #333; */
  padding: 30rpx;
}
.txt-box .text{
  font-size: 24rpx;
  line-height: 45rpx;
}
.btn-box{
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #333;
}
.btn{
  width: 49%;
  border-right: 1rpx solid #333;
  text-align: center;
  float: left;
  display: inline-block;
  font-size: 28rpx;
}
.btn:last-child{
  border: none;
}
.dh{
  width: 29rpx;
  height: 29rpx;
  display: inline-block;
  margin-right: 20rpx;
  vertical-align: sub;
}
.phone{
  width: 20rpx;
  height: 31rpx;
  display: inline-block;
  margin-right: 20rpx;
  vertical-align: sub;
}
.addrtxt{
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: rgba(0, 0, 0, .5);
  display: inline-block;
  border-radius: 29rpx; 
  padding: 0 0rpx 0 30rpx;
  position: absolute;
  top: 35rpx;
  left: 35rpx;
  z-index: 999999;
}
.mapaddr{
  width: 20rpx;
  height: 24rpx;
  display: inline-block;
  margin-right: 50rpx;
  vertical-align: middle;
}
.city-picker{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  height: 300px;
  width: 100%;
  /* background: red; */
  color: #000;
  background-color: #fff;
}
.city-picker view{
  color: #000;
  text-align: center;
}